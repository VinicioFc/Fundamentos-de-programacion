#include <iostream>

using namespace std;

int main()
{
    float A,B,C,S;
    bool R1,R2,R3,R4,R5,R6,R7,R8;

    cout << "Ingrese el primer valor DE A " << endl;
    cin >> A;
    cout << "Ingrese el primer valor DE B " << endl;
    cin >> B;
    cout << "Ingrese el primer valor DE C " << endl;
    cin >> C;

    R1 = ( A+B )   ;
    R2 = (R1 == C );
    R3 = ( C != 0 );
    R4 = (B-C)     ;
    R5 = R4 >= 19  ;
    R6 = !R2       ;
    R7 = R3 && R5  ;
    R8 = R6 || R7 ;

    cout << " El resultado es " << R8 << endl;

    return 0;
}
 
